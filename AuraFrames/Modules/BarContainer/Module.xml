<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

  <Script file="Module.lua" />
  <Script file="Prototype.lua" />

  <Frame name="AuraFramesBarContainerTemplate" parent="UIParent" frameStrata="LOW" toplevel="true" virtual="true" movable="true">
  </Frame>

  <Frame name="AuraFramesBarTemplate" virtual="true">
    <Frames>
      <Button name="$parentButton">
        <Size x="36" y="36"/>
        <Layers>
          <Layer level="BACKGROUND">
            <Texture name="$parentIcon">
              <Size>
                <AbsDimension x="36" y="36"/>
              </Size>
            </Texture>
          </Layer>
          <Layer level="OVERLAY">
            <Texture name="$parentBorder" file="Interface\Buttons\UI-Debuff-Overlays" alphaMode="ADD">
              <Size>
                <AbsDimension x="36" y="36"/>
              </Size>
              <Anchors>
                <Anchor point="CENTER"/>
              </Anchors>
              <TexCoords left="0.296875" right="0.5703125" top="0" bottom="0.515625"/>
            </Texture>
          </Layer>
        </Layers>
      </Button>
    </Frames>
    <Layers>
      <Layer level="BACKGROUND">
        <Texture name="$parentTextureBackground">
          <Color r="1.0" g="1.0" b="1.0" a="1.0"/>
        </Texture>
        <Texture name="$parentButtonBackground">
          <Color r="1.0" g="1.0" b="1.0" a="1.0"/>
          <Size>
            <AbsDimension x="36" y="36"/>
          </Size>
          <Anchors>
            <Anchor point="CENTER" relativeTo="$parentButtonIcon" />
          </Anchors>
        </Texture>
      </Layer>
      <Layer level="BORDER">
        <Texture name="$parentTexture" />
      </Layer>
      <Layer level="ARTWORK">
        <Texture name="$parentSpark" file="Interface\CastingBar\UI-CastingBar-Spark" alphaMode="ADD">
          <Size>
            <AbsDimension x="36" y="100"/>
          </Size>
        </Texture>
      </Layer>
      <Layer level="OVERLAY">
        <FontString name="$parentText" justifyH="RIGHT" />
        <FontString name="$parentDuration" hidden="true" />
      </Layer>
    </Layers>
  </Frame>

</Ui>
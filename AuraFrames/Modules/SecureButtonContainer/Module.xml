<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

  <Script file="Module.lua" />
  <Script file="Prototype.lua" />

  <Button name="AuraFramesSecureButtonTemplate" inherits="SecureActionButtonTemplate" virtual="true" hidden="true">
    <Attributes>
      <Attribute name="type" value="cancelaura"/>
    </Attributes>
    <Size>
      <AbsDimension x="36" y="36"/>
    </Size>
    <Attributes>
      <Attribute name="type" value="cancelaura"/>
    </Attributes>
    <Layers>
      <Layer level="BORDER">
        <Texture name="$parentIcon" />
      </Layer>
      <Layer level="ARTWORK">
        <FontString name="$parentCount" hidden="true" justifyH="RIGHT" />
        <FontString name="$parentDuration" hidden="true" />
      </Layer>
      <Layer level="OVERLAY">
        <Texture name="$parentBorder" file="Interface\Buttons\UI-Debuff-Overlays" alphaMode="ADD">
          <Size>
            <AbsDimension x="36" y="36"/>
          </Size>
          <Anchors>
            <Anchor point="CENTER"/>
          </Anchors>
          <TexCoords left="0.296875" right="0.5703125" top="0" bottom="0.515625"/>
        </Texture>
      </Layer>
    </Layers>
    <Frames>
      <Cooldown name="$parentCooldown" level="BORDER" hidden="true">
        <Size>
          <AbsDimension x="36" y="36"/>
        </Size>
        <Anchors>
          <Anchor point="CENTER" />
        </Anchors>
      </Cooldown>
    </Frames>
    <Scripts>
      <OnLoad>
        LibStub("AceAddon-3.0"):GetAddon("AuraFrames"):GetModule("SecureButtonContainer"):RegisterButton(self);
      </OnLoad>
    </Scripts>
  </Button>

</Ui>